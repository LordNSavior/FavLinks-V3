{
  "name": "favlinks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm --prefix backend run dev",
    "build": "vite build",
    "lint": "eslint .",
